#APP-USERLIST(style="width: 100%; height: 100%")
    .g-page(v-loading="loading")
        .g-title 管理员列表
        el-button-group()
            el-button(icon="el-icon-plus", @click="add") 新增
            el-button(icon="el-icon-delete", @click="batchDelete") 删除
            el-button(icon="el-icon-refresh-right" @click="refresh") 刷新
        el-table.g-table(:data="listData", stripe)
            el-table-column(type="index", width="60")
            el-table-column(type="selection", width="60")
            el-table-column(prop="username", label="用户名")
            el-table-column(prop="updateTime", label="修改时间", width="200", :formatter="$util.formatter.mongoTime")
            el-table-column(prop="createTime", label="创建时间", width="200", :formatter="$util.formatter.mongoTime")

        el-dialog(title="管理员信息", :visible.sync="dialogVisible")
            el-form(:model="formData")
                el-form-item(label="用户名")
                    input(v-model="formData.username", type="text")
                el-form-item(label="密码")
                    input(v-model="formData.password", type="password")
            .dialog-footer(slot="footer")
                el-button(@click="dialogVisible = false") 关闭
                el-button(type="primary", @click="save")